{
  "name": "felida",
  "version": "0.2.4",
  "description": "A lightweight chromium browser using Electron.js",
  "main": "compiled-electron/index.js",
  "scripts": {
    "compile:electron": "rimraf compiled-electron/ && tsc -p tsconfig.electron.json",
    "compile:interface": "rollup -c --bundleConfigAsCjs",
    "compile": "npm run compile:interface && npm run compile:electron",
    "start:electron:dev": "DEV=true electron compiled-electron/index.js",
    "start": "npm run start:electron:dev",
    "build": "npx electron-builder -wl",
    "watch": "nodemon -i ./data --exec npm run compile:electron"
  },
  "author": "raluvy95",
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.26.5",
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "draggabilly": "^3.0.0",
    "electron-context-menu": "^3.6.1",
    "jquery": "^3.7.0",
    "luxon": "^3.3.0",
    "node-fetch": "^2.6.1",
    "sass": "^1.62.1",
    "svelte-preprocess": "^5.0.3"
  },
  "build": {
    "appId": "net.raluvy95.felida",
    "productName": "Felida Browser",
    "copyright": "Copyright Â© 2023 raluvy95",
    "icon": "icon.png",
    "directories": {
      "buildResources": "resources",
      "output": "build"
    },
    "win": {
      "target": "zip",
      "icon": "icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Internet",
      "icon": "icon.png"
    }
  },
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raluvy95/FelidaBrowser.git"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/raluvy95/FelidaBrowser/issues"
  },
  "homepage": "https://github.com/raluvy95/FelidaBrowser#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@tsconfig/svelte": "^4.0.1",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "electron": "^24.4.0",
    "electron-builder": "^24.4.0",
    "eslint": "^8.41.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.20",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.5",
    "svelte": "^3.59.1",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "private": true
}
